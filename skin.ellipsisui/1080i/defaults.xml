<includes>
    <default type="image">
        <left>0</left>
        <top>0</top>
        <width>0</width>
        <height>0</height>
        <aspectratio>stretch</aspectratio>
    </default>
    <default type="label">
        <left>0</left>
        <top>0</top>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <shadowcolor />
        <align>left</align>
    </default>
    <default type="fadelabel">
        <left>0</left>
        <top>0</top>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <shadowcolor />
