        <textcolor>Text</textcolor>
        <disabledcolor>DisabledText</disabledcolor>
        <aligny>top</aligny>
        <textoffsetx>22</textoffsetx>
        <textoffsety>14</textoffsety>
    </default>
    <default type="mover">
        <left>0</left>
        <top>0</top>
        <width>0</width>
        <height>0</height>
