        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
    </default>
    <default type="resize">
        <left>0</left>
        <top>0</top>
        <width>0</width>
        <height>0</height>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
    </default>
    <default type="scrollbar" id="60">
        <texturesliderbackground>controls\scrollbar_back.png</texturesliderbackground>
        <texturesliderbar>controls\scrollbar_bar_nf.png</texturesliderbar>
        <texturesliderbarfocus>controls\scrollbar_bar_f.png</texturesliderbarfocus>
        <textureslidernib />
        <textureslidernibfocus />
        <showonepage>false</showonepage>
        <orientation>vertical</orientation>
    </default>
