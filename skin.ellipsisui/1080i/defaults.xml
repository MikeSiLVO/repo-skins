        <radiowidth>24</radiowidth>
        <radioheight>24</radioheight>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
        <textureradioonfocus>controls\radiobutton_f.png</textureradioonfocus>
        <textureradioonnofocus>controls\radiobutton_nf.png</textureradioonnofocus>
        <textureradioondisabled colordiffuse="40FFFFFF">controls\radiobutton_f.png</textureradioondisabled>
        <textureradiooffdisabled colordiffuse="40FFFFFF">controls\radiobutton_nf.png</textureradiooffdisabled>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="slider">
        <controloffsetx>75</controloffsetx>
        <controloffsety>0</controloffsety>
        <orientation>horizontal</orientation>
    </default>
    <default type="sliderex">
        <left>0</left>
        <top>0</top>
        <width>0</width>
        <height>0</height>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
        <sliderwidth>150</sliderwidth>
        <sliderheight>32</sliderheight>
        <texturesliderbar>controls\slider_back.png</texturesliderbar>
        <textureslidernib>controls\slider_nib.png</textureslidernib>
        <textureslidernibfocus>controls\slider_nib_focus.png</textureslidernibfocus>
