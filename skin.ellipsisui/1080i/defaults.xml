        <top>0</top>
        <width>0</width>
        <height>0</height>
        <texturebg>controls\toast-progress-empty.png</texturebg>
        <lefttexture />
        <midtexture>controls\toast-progress-full.png</midtexture>
        <righttexture />
        <overlaytexture />
    </default>
    <default type="spincontrolex">
        <left>0</left>
        <top>0</top>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <disabledcolor>DisabledText</disabledcolor>
        <shadowcolor />
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
        <textureup>controls\sortup_nf.png</textureup>
        <texturedown>controls\sortdown_nf.png</texturedown>
        <textureupfocus>controls\sortup_f.png</textureupfocus>
        <texturedownfocus>controls\sortdown_f.png</texturedownfocus>
