        <textoffsetx>22</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrol">
        <left>0</left>
        <top>0</top>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <disabledcolor>DisabledText</disabledcolor>
        <shadowcolor />
