        <reverse>yes</reverse>
        <textoffsetx>22</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="radiobutton">
        <left>0</left>
        <top>0</top>
        <width>0</width>
        <height>0</height>
