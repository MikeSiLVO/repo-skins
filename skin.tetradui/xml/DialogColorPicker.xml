				<onright condition="Control.IsVisible(60)">60</onright>
				<onright condition="!Control.IsVisible(60)">6</onright>
				<onleft>6</onleft>
				<ondown>6</ondown>
				<onup>6</onup>
				<animation effect="slide" end="210,0" condition="!Control.IsVisible(60)">Conditional</animation>
