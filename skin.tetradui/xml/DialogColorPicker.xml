				<width>320</width>
				<height>35</height>
				<font>font12</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>orange</textcolor>
				<label>$LOCALIZE[31201] $INFO[Container(6).CurrentPage]/$INFO[Container(6).NumPages]</label>
				<visible>Control.IsVisible(60) + Control.HasFocus(60)</visible>
			</control>
