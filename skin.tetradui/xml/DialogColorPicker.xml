						<width>82</width>
						<height>54</height>
						<texture border="10">buttons/button-dialog-nofocus.png</texture>
					</control>
					<control type="image">
						<top>5</top>
						<left>5</left>
						<width>82</width>
						<height>54</height>
						<texture border="10">white.png</texture>
