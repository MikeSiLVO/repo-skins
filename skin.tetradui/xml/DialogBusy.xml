<?xml version="1.0" encoding="UTF-8"?>
<window>
	<include>dialogeffect</include>
	<coordinates>
		<system>0</system>
		<left>1040</left>
		<top>-3</top>
	</coordinates>
	<controls>
		<control type="group">
			<animation effect="slide" start="0,0" end="0,10" time="200" condition="Window.IsVisible(FullscreenVideo) | Window.IsVisible(Visualisation)">conditional</animation>
			<control type="image">
				<description>Media Overlay Image</description>
				<width>200</width>
				<height>85</height>
				<texture border="20,20,20,30">dialogs/Dialog2.png</texture>
			</control>
			<control type="grouplist">
				<left>10</left>
				<top>20</top>
				<width>160</width>
				<height>50</height>
				<itemgap>20</itemgap>
				<orientation>horizontal</orientation>
				<align>center</align>
				<control type="image">
					<description>Busy Image</description>
					<width>35</width>
					<height>35</height>
					<aspectratio>keep</aspectratio>
					<texture>busy.png</texture>
					<animation effect="rotate" start="0" end="360" center="auto" time="1200" loop="true" condition="true">conditional</animation>
				</control>
				<control type="label">
					<width max="120">auto</width>
					<height>35</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<label>31302</label>
					<textcolor>orange</textcolor>
				</control>
			</control>
		</control>
	</controls>
</window>